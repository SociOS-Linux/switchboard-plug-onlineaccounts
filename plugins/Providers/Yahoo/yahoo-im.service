<?xml version="1.0" encoding="UTF-8" ?>
<service id="yahoo-im">
  <type>IM</type>
  <name>Yahoo!</name>
  <icon>yahoo</icon>
  <provider>yahoo</provider>

  <!-- default settings (account settings have precedence over these) -->
  <template>
    <group name="auth">
      <setting name="method">password</setting>
      <setting name="mechanism">password</setting>
    </group>
  </template>

</service>